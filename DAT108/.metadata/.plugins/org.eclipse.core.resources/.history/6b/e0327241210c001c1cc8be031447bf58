package hvl.no;

public class Burger {
	
	int burgerID;

	public Burger(int ID) {
		this.burgerID = ID;
	}

	public int getBurgerNr() {
		return burgerID;
	}

	public void setBurgerNr(int burgerNr) {
		this.burgerID = burgerNr;
	}
	
	public String toString() {
		return "(|" + burgerID + "|)";
	}

}
